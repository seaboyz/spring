<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->


    <bean id="triangle" class="com.webdev.spring.Triangle" scope="prototype"></bean>

    <bean id="pointA" class="com.webdev.spring.Point">
        <property name="x" value="0" />
        <property name="y" value="0" />
    </bean>

    <bean id="pointB" class="com.webdev.spring.Point">
        <property name="x" value="-20" />
        <property name="y" value="10" />
    </bean>

    <bean id="pointC" class="com.webdev.spring.Point">
        <property name="x" value="20" />
        <property name="y" value="10" />
    </bean>


</beans>